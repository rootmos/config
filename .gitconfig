    ryck = pull
    knuffa = push
    #hämta = fetch
    gren = branch
    #förgrena = branch
    #förbinda = commit
    ympa = rebase
    sammanfoga = merge
    #göm = stash
    klandra = blame
    #märke = tag
    #märk = tag
    ba = branch --sort=committerdate --all -vv
    ls = ls-files
[color]
    ui = auto
